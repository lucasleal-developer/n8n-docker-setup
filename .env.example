# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_strong_postgres_password_here
POSTGRES_DB=n8n
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=your_strong_n8n_password_here

# n8n Configuration
N8N_ENCRYPTION_KEY=your_encryption_key_here_32_characters_long
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your_admin_password_here

# Webhook Configuration
WEBHOOK_URL=http://localhost:5679

# Timezone Configuration
GENERIC_TIMEZONE=America/Sao_Paulo

# Redis Configuration (optional, using defaults)
# QUEUE_BULL_REDIS_HOST=redis
# QUEUE_BULL_REDIS_PORT=6379
# QUEUE_BULL_REDIS_PASSWORD=
# QUEUE_BULL_REDIS_DB=0